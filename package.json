{
  "name": "sensor-tag-adapter",
  "display_name": "SensorTag",
  "version": "0.1.4",
  "description": "Connect your SensorTags",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-hellhake/sensor-tag-adapter.git"
  },
  "author": "Tim Hellhake",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/tim-hellhake/sensor-tag-adapter/issues"
  },
  "homepage": "https://github.com/tim-hellhake/sensor-tag-adapter#readme",
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "index.js",
    "sensor-tag-adapter.js",
    "manifest.json"
  ],
  "moziot": {
    "api": {
      "min": 2,
      "max": 2
    },
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "schema": {
      "type": "object",
      "required": [
        "pollInterval"
      ],
      "properties": {
        "pollInterval": {
          "type": "integer",
          "title": "The number of seconds between two polls"
        }
      }
    }
  },
  "dependencies": {
    "sensortag": "github:tim-hellhake/node-sensortag#switch-to-abandonware-noble"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1"
  }
}
